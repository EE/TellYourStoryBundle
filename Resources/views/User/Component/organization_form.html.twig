<div class="row">
    <form class="form-horizontal form-tys" action="{{ path('eetys_user_set_organization') }}" method="post" {{ form_enctype(organizationForm) }}>
        <input type="hidden" name="_method" value="PUT"/>
        <div class="span12">
            {{ form_errors(organizationForm) }}
            {{ form_errors(organizationForm.organization) }}
        </div>

        <div class="span4">{{ form_label(organizationForm.organization) }}</div>
        <div class="span4">{{ form_widget(organizationForm.organization) }}</div>

        <div class="span4">
            <button class="btn btn-tys btn-small" type="submit">{{ 'btn.add.name' | trans }}<i class="icon-chevron-right icon-white"></i></button>
        </div>
        {{ form_end(organizationForm) }}
</div>